
@{
    ViewBag.Title = "ThankYou";
}

<div class="col-md-12 col-md-offset-3">

    @if (TempData["FirstName"] != null)
    {
        <h1>Hi @TempData["FirstName"].ToString(), Thank You for sending your information.</h1>
    }
    else
    {
        <h1>No Information Found!</h1>
    }
    
</div>

